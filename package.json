{
  "name": "nodejs-mongo",
  "version": "0.1.0",
  "description": "An example nodejs/mongo app dockerized for local development",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"cyan.bold,green.bold\" \"npm run -s watch-ts\" \"npm run -s watch-node\"",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "watch-test": "npm run -s test -- --watchAll"
  },
  "author": "Alex Lapinski <contact@alexlapinski.name>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "mongoose": "^4.9.9",
    "pg": "^6.2.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.11.0",
    "ts-jest": "^22.4.5",
    "ts-node": "^6.0.3",
    "tsdoc": "0.0.4",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
